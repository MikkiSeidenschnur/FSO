     xmlns:vann="http://purl.org/vocab/vann/"
     xmlns:terms="http://purl.org/dc/terms/">
    <owl:Ontology rdf:about="https://w3id.org/fso#">
        <owl:versionIRI rdf:resource="https://w3id.org/fso/0.0.1"/>
        <terms:contributor>All contributing members of the W3C Linked Building Data Community Group</terms:contributor>
        <terms:creator>Ali Kücükavci</terms:creator>
        <terms:creator>Mads Holten Rasmussen</terms:creator>
        <terms:creator>Ville Kukkonen</terms:creator>
        <terms:description xml:lang="en">Some description in english</terms:description>
        <terms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2020-01-01T12:00:00</terms:issued>
        <terms:license rdf:resource="https://creativecommons.org/licenses/by/1.0/"/>
        <terms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2019-11-18T12:00:00</terms:modified>
        <terms:title xml:lang="en">The Flow Systems Ontology (FSO)</terms:title>
        <vann:preferredNamespacePrefix>fso</vann:preferredNamespacePrefix>
        <vann:preferredNamespaceUri rdf:resource="https://w3id.org/fso#"/>
        <owl:priorVersion rdf:resource="https://w3id.org/fso/0.0.1"/>
        <owl:versionInfo>0.0.1</owl:versionInfo>
    </owl:Ontology>
    


